<%- include ("../templates/encabezado") %>
<%- include ("../templates/menuUsu") %>
<div class="container">
    <form action="/nuevaNoticia1" method="post" enctype="multipart/form-data">
       <div class="card">
        <div class="card-header">
            <h1>Nueva Noticia</h1>
        </div>
        <div class="card-body">
            
            <input class="form-control mb-3" type="text" name="titulo" id="titulo" placeholder="Titulo" autofocus>
            <input class="form-control mb-3" type="text" name="historia" id="historia" placeholder="Historia" >
            <input class="form-control mb-3" type="file" name="foto" id="foto" placeholder="Selecciona tu foto">


        </div>
        <div class="card-footer">
            <button class="btn btn-primary mb-3" type="submit">Guardar Noticia</button>
            <button class="btn btn-secondary mb-3" type="reset">Limpiar campos</button>
        </div>

       </div> 
    </form>
</div>
